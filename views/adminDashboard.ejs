<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Dashboard</title>
</head>

<body>
    <h2>DashBoard</h2>
    <div>
        <a href="/admin/logout">Logout</a>
    </div>
    <table>
        <tr>
            <th>No</th>
            <th>Name</th>
            <th>Email</th>
            <th>Phone Number</th>
            <th></th>
            <th> </th>
        </tr>
        <% users.forEach((user,i)=> { %>
            <tr>
                <td>
                    <%= i+1 %>
                </td>
                <td>
                    <%= user.name %>
                </td>
                <td>
                    <%= user.email %>
                </td>
                <td>
                    <%= user.phoneNum %>
                </td>
                <td>
                    <div>
                        <a href="/admin/edit/<%= user._id %>">Edit</a>
                    </div>
                </td>
                <td>
                    <div>
                        <a href="/admin/delete/<%= user._id %>">Delete</a>
                    </div>

                    </th>
            </tr>
            <% }) %>
    </table>
</body>

</html>